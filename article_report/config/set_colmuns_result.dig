+function_columns_result:
  td>:
  query: |
    SELECT
      ARRAY_JOIN(ARRAY_AGG(column_name), ',') AS set_columns
    FROM
      information_schema.columns
    WHERE
      table_schema = '${td.database}'
      AND table_name = '${tbl}'
      AND column_name <> 'time'
    GROUP BY
      table_name
  store_last_results: true